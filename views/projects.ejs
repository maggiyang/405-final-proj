<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
</head>
<body>
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <a href="/admin/projects">Projects</a>
        </div>
        <div class="col-md-6">
            <a href="/admin/users">Users</a>
        </div>
    </div>
    <a href="/admin/create-project">Create New Project</a>
    <table class="table">
        <tr>
            <th>id</th>
            <th>title</th>
            <th>description</th>
            <th>category</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
        <% projects.forEach(function(project){ %>
            <tr>
                <td><%= project.id %></td>
                <td><%= project.title %></td>
                <td><%= project.description %></td>
                <td><%= project.category %></td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
        <% })  %>

    </table>
</div>
</body>
</html>